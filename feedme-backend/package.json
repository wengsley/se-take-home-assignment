{
  "name": "mcdonalds-order-controller",
  "version": "1.0.0",
  "description": "McDonald's Order Controller API for automated cooking bots",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "start:api": "node dist/index.js",
    "start:cli": "tsx src/cli.ts",
    "dev": "tsx watch src/index.ts",
    "dev:api": "tsx watch src/index.ts",
    "dev:cli": "tsx watch src/cli.ts",
    "test": "tsx src/**/*.test.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy"
  },
  "keywords": ["api", "order-management", "express", "typescript"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "@prisma/client": "^5.7.1",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.5",
    "@types/cors": "^2.8.17",
    "prisma": "^5.7.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
